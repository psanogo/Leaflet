my_map <- leaflet() %>% addTiles()




for (b in -30:30)
{
    for (a in 30:(-30))
    {
        
        if (((a*0.05)^2+(b*0.1)^2-1)^3-(a*0.05)^2*(b*0.1)^3 <= 0 ) 
        my_map <- addMarkers(my_map, lat=17° 34' 26.16" N,lng=-3° 59' 9.99" W, popup="Hi there. ;-) - 14 Avril 2023")
    }
}
        
my_map
